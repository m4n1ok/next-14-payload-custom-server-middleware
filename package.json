{
  "name": "middleware-not-running-custom-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
      "build": "cross-env NODE_ENV=production pnpm run build:payload && pnpm run build:server && pnpm run copyfiles && pnpm run build:next",
      "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/cms/payload.config.js NEXT_BUILD=true node dist/server.js",
      "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload build",
      "build:server": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.json",
      "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,js,json}\" dist/",
      "dev": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts nodemon",
      "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload generate:graphQLSchema",
      "generate:migrations": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload generate:types",
      "generate:secret": "node -e \"console.log(require('crypto').randomBytes(32).toString('hex'))\"",
      "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload generate:types",
      "payload": "cross-env PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts payload",
      "seed": "cross-env PAYLOAD_SEED=true PAYLOAD_CONFIG_PATH=src/cms/payload.config.ts ts-node --project tsconfig.server.json -r tsconfig-paths/register src/server.ts",
      "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/cms/payload.config.js NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@nouance/payload-better-fields-plugin": "^1.3.5",
    "@payloadcms/bundler-webpack": "^1.0.6",
    "@payloadcms/db-mongodb": "^1.4.3",
    "@payloadcms/plugin-redirects": "^1.0.1",
    "@payloadcms/plugin-seo": "^2.2.1",
    "@payloadcms/richtext-lexical": "^0.7.0",
    "@types/nodemailer-sendgrid": "^1.0.3",
    "cross-env": "^7.0.3",
    "escape-html": "^1.0.3",
    "express": "^4.18.3",
    "next": "^14.1.1",
    "payload": "^2.11.2",
    "payload-admin-bar": "^1.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "^1.1.1",
    "@svgr/webpack": "^8.1.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.5",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
